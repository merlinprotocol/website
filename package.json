{
  "private": true,
  "scripts": {
    "start": "cross-env UMI_ENV=dev umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@apollo/client": "^3.5.10",
    "@metamask/onboarding": "^1.0.1",
    "@usedapp/core": "^1.0.0",
    "@web3-react/core": "^6.1.9",
    "axios": "^0.26.0",
    "dayjs": "^1.10.7",
    "graphql": "^16.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-use": "^17.3.2",
    "umi": "^3.5.20",
    "web3": "^1.7.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.181",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
